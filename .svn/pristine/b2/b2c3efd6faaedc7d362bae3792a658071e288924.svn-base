package com.bomweb.transaction.xml;

import javax.xml.bind.annotation.XmlAttribute;
import javax.xml.bind.annotation.XmlValue;

/**
 * @author SACHIN
 *
 */
public class SecKey {

	private String ci;
	private String ki;
	private String value;

	/**
	 * @return the ci
	 */
	@XmlAttribute
	public String getCi() {
		return ci;
	}

	/**
	 * @param ci
	 *            the ci to set
	 */
	public void setCi(String ci) {
		this.ci = ci;
	}

	/**
	 * @return the ki
	 */
	@XmlAttribute
	public String getKi() {
		return ki;
	}

	/**
	 * @param ki
	 *            the ki to set
	 */
	public void setKi(String ki) {
		this.ki = ki;
	}

	/**
	 * @return the value
	 */
	@XmlValue
	public String getValue() {
		return value;
	}

	/**
	 * @param value
	 *            the value to set
	 */
	public void setValue(String value) {
		this.value = value;
	}

}
