package com.bomweb.bbps.xml;

import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;

/**
 * @author SACHIN
 *
 */
@XmlRootElement(name = "com.bomweb.bbps.BillStatusResponse")
public class PayStatusResponse {

	private ErrorMessages errorMessages;
	private Fillers fillers;
	private String requestId;
	private String bbpsReferenceNo;
	private String reason;
	private String timeStamp;
	private String transactionId;
	private String valid;

	/**
	 * @return the requestId
	 */
	public String getRequestId() {
		return requestId;
	}

	/**
	 * @param requestId
	 *            the requestId to set
	 */
	@XmlElement(name = "requestId")
	public void setRequestId(String requestId) {
		this.requestId = requestId;
	}

	/**
	 * @return the valid
	 */
	public String getValid() {
		return valid;
	}

	/**
	 * @param valid
	 *            the valid to set
	 */
	@XmlElement(name = "valid")
	public void setValid(String valid) {
		this.valid = valid;
	}

	public ErrorMessages getErrorMessages() {
		return errorMessages;
	}

	@XmlElement(name = "errorMessages")
	public void setErrorMessages(ErrorMessages errorMessages) {
		this.errorMessages = errorMessages;
	}

	public Fillers getFillers() {
		return fillers;
	}

	@XmlElement(name = "fillers")
	public void setFillers(Fillers fillers) {
		this.fillers = fillers;
	}

	/**
	 * @return the bbpsReferenceNo
	 */
	public String getBbpsReferenceNo() {
		return bbpsReferenceNo;
	}

	/**
	 * @param bbpsReferenceNo
	 *            the bbpsReferenceNo to set
	 */
	@XmlElement(name = "bbpsReferenceNo")
	public void setBbpsReferenceNo(String bbpsReferenceNo) {
		this.bbpsReferenceNo = bbpsReferenceNo;
	}

	/**
	 * @return the reason
	 */
	public String getReason() {
		return reason;
	}

	/**
	 * @param reason
	 *            the reason to set
	 */
	@XmlElement(name = "reason")
	public void setReason(String reason) {
		this.reason = reason;
	}

	/**
	 * @return the timeStamp
	 */
	public String getTimeStamp() {
		return timeStamp;
	}

	/**
	 * @param timeStamp
	 *            the timeStamp to set
	 */
	@XmlElement(name = "timeStamp")
	public void setTimeStamp(String timeStamp) {
		this.timeStamp = timeStamp;
	}

	/**
	 * @return the transactionId
	 */
	public String getTransactionId() {
		return transactionId;
	}

	/**
	 * @param transactionId
	 *            the transactionId to set
	 */
	@XmlElement(name = "transactionId")
	public void setTransactionId(String transactionId) {
		this.transactionId = transactionId;
	}

}
